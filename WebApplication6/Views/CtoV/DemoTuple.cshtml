@using WebApplication6.Models;
@model Tuple<List<Product>, List<Category>, List<Supplier>>

@{
    ViewBag.Title = "DemoTuple";
}

<h2>DemoTuple</h2>

<h3>ProductName</h3>\
<ul>
    @* Item1 is List<Product>*@
    @foreach(var item in Model.Item1)
    {
        <li>
            @item.ProductName
        </li>
    }
</ul>

<h3>CategoryName</h3>
<ul>
    @foreach(var item in Model.Item2)
    {
        <li>
            @item.CategoryName
        </li>
    }
</ul>

<h3>SupplierName</h3>
<ul>
    @foreach(var item in Model.Item3)
    {
        <li>
            @item.CompanyName
        </li>
    }
</ul>